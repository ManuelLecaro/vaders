{
    "name": "Pescador bueno",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js": [ "js/popup.js"]
        }
    ],
    
    "browser_action":{
        "default_icon": "images/cctv.png",
        "default_popup":"html/popup.html",
        "default_title":"Reto"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    
    "options_page": "html/options.html"

}